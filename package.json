{
  "name": "qa_dashboard",
  "version": "1.0.0",
  "description": "DevRev QA Dashboard - View and manage issues and tickets from DevRev",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --exec node --import tsx server/index.ts",
    "client:dev": "vite",
    "build": "tsc && vite build",
    "start": "node --import tsx server/index.ts",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "antd": "^5.26.6",
    "axios": "^1.12.0",
    "better-sqlite3": "^11.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.28.0",
    "recharts": "^3.7.0",
    "shrink-ray-current": "^4.1.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/react": "^17.0.71",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "sass": "^1.77.8",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
